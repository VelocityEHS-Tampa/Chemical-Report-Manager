<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - crm.chemtelinc.com</title>
    <link rel="stylesheet" href="~/Content/bootstrap.css" />
    <link rel="stylesheet" href="~/Content/Site.css" />
    <script src="~/scripts/jquery.min.js"></script>
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container">
                <a class="navbar-brand" href="@Url.Action("Index", "Home")">Chemical Report Manager</a>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-dark" id="GLOTCEQMenu" href="#">GLO / TCEQ</a>
                            <div class="NavMenuContainer">
                                <ul class="NavMenu" id="GLOTCEQOptions">
                                    <li class="MenuOptionHeader"><h5>GLO</h5></li>
                                    <li><a href="@Url.Action("GLOReport", "GLO")">GLO Reports</a></li>
                                    <li><a href="~/PDFs/GLO/Texas General Land Office Internal Script and Helpful Hints.pdf" target="_blank">Internal Script and Hints</a></li>
                                    <li><a href="~/PDFs/GLO/GLO Jurisdiction Matrix.pdf" target="_blank">View Spill Matrix</li>
                                    <li><a href="~/PDFs/GLO/GLO Classification Tool for Materials and Origins.pdf" target="_blank">Classification Tool</a></li>
                                    <li><a href="~/PDFs/GLO/After Hours Contacts.pdf" target="_blank">After Hours Contacts</a></li>
                                    <li>&nbsp;</li>
                                    <li class="MenuOptionHeader"><h5>TCEQ</h5></li>
                                    <li><a href="@Url.Action("TCEQReport", "TCEQ")">TCEQ Report</a></li>
                                    <li><a href="~/PDFs/TCEQ/TCEQ After Hours Complaint Line Script.pdf" target="_blank">TCEQ Complaint Line Script</a></li>
                                    <li><a href="@Url.Action("TCEQReportLog", "TCEQ")">TCEQ Log</a></li>
                                    <li><a href="~/PDFs/TCEQ/TCEQ Regional ER Contact List.pdf" target="_blank">TCEQ After Hours Contacts</a></li>
                                </ul>
                            </div>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" id="CrestwoodMenu" href="#">Crestwood</a>
                            <div class="NavMenuContainer">
                                <ul class="NavMenu" id="CrestwoodOptions">
                                    <li class="MenuOptionHeader"><h5>Reports</h5></li>

                                    <li><a href="@Url.Action("GeneralIncident", "Crestwood")">General Incident</a></li>
                                    <li><a href="~/PDFs/Crestwood/Enviance Guide.pdf" target="_blank">Enviance Guide</a></li>
                                    <li><a href="@Url.Action("PipelineReport", "Crestwood")">Pipeline</a></li>
                                    <li><a href="@Url.Action("EightOneOneReport", "Crestwood")">811 Reports</a></li>
                                    <li><a href="@Url.Action("LockedGateReport", "Crestwood")">Locked Gate Access</a></li>
                                    <li>&nbsp;</li>

                                    <li class="MenuOptionHeader"><h5>Logs</h5></li>

                                    <li><a href="@Url.Action("CrestwoodLogs", "Crestwood", new { SelectedLog = "GenInc" })">General Incident</a></li>
                                    <li><a href="@Url.Action("CrestwoodLogs", "Crestwood", new { SelectedLog = "Pipeline" })">Pipeline</a></li>
                                    <li><a href="@Url.Action("CrestwoodLogs", "Crestwood", new { SelectedLog = "811" })">811 Log</a></li>
                                    <li><a href="@Url.Action("CrestwoodLogs", "Crestwood", new { SelectedLog = "LockedGate" })">Locked Gate Access</a></li>
                                    <li>&nbsp;</li>

                                    <li class="MenuOptionHeader"><h5>Manager</h5></li>

                                    <li><a href="@Url.Action("ManagerTools", "Crestwood", new { SelectedTool = "HSER" })">HSER Members</a></li>
                                    <li><a href="@Url.Action("ManagerTools", "Crestwood", new { SelectedTool = "PipelineMembers" })">Pipeline Members</a></li>
                                    <li><a href="@Url.Action("ManagerTools", "Crestwood", new { SelectedTool = "TransportList" })">Transportation List</a></li>
                                    <li><a href="@Url.Action("ManagerTools", "Crestwood", new { SelectedTool = "AllNotifications" })">All Notifcations</a></li>
                                    <li>&nbsp;</li>

                                    <li class="MenuOptionHeader"><h5>Other</h5></li>

                                    <li><a href="~/PDFs/Crestwood/Crestwood Helpful Hints.pdf" target="_blank">Helpful Hints</a></li>
                                    <li>Phone Numbers</li>
                                    <li><a href="~/PDFs/Crestwood/Crestwood Scripts Rotations and Notification.pdf" target="_blank">Script</a></li>
                                </ul>
                            </div>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" id="OtherReportsMenu" href="#">Other Reports/Logs</a>
                            <div class="NavMenuContainer">
                                <ul class="NavMenu" id="OtherReportsOptions">
                                    <li class="MenuOptionHeader"><h5>Incident Reports</h5></li>

                                    <li><a href="@Url.Action("GeneralIncident", "OtherReports")">General Incident Report</a></li>
                                    <li><a href="@Url.Action("CleanShotReport", "OtherReports")">Clean Shot Incident Report</a></li>
                                    <li><a href="@Url.Action("ShellOilReport", "OtherReports")">Shell Oil Incident Report</a></li>
                                    <li>&nbsp;</li>

                                    <li class="MenuOptionHeader"><h5>Neutralization and FAQ</h5></li>

                                    <li><a href="~/PDFs/Other Reports/Clean Shot Sulfuric Acid and Buster Neutralization and Common Consumer Questions.pdf" target="_blank">Clean Shot Sulfuric Acid/Buster</a></li>
                                    <li><a href="~/PDFs/Other Reports/Clean Shot Instant Drain Opener Neutralization and Common Consumer Questions.pdf" target="_blank">Clean Shot Instant Drain Opener</a></li>
                                    <li>&nbsp;</li>

                                    <li class="MenuOptionHeader"><h5>Incident Logs</h5></li>

                                    <li><a href="@Url.Action("OtherReportsLogs", "OtherReports", new { LogSelected = "GenIncLog"})">General Incident Log</a></li>
                                    <li><a href="@Url.Action("OtherReportsLogs", "OtherReports", new { LogSelected = "CleanShot"})">Clean Shot Incident Log</a></li>
                                    <li><a href="@Url.Action("OtherReportsLogs", "OtherReports", new { LogSelected = "ShellOil"})">Shell Oil Incident Log</a></li>
                                </ul>
                            </div>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" href="@Url.Action("MSDSSearch", "Home")">MSDS Search</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" id="ManagerToolsMenu" href="#">Manager Tools</a>
                            <div class="NavMenuContainer">
                                <ul class="NavMenu" id="ManagerToolsOptions">
                                    <li><a href="@Url.Action("IDEditing", "ManagerTools", new { IDsSelected = "NewGLO" })">New GLO IDs</a></li>
                                    <li><a href="@Url.Action("IDEditing", "ManagerTools", new { IDsSelected = "OldGLO"})">Old GLO IDs</a></li>
                                    <li><a href="@Url.Action("ManagerToolsEtc", "ManagerTools", new { OtherToolSelected = "GLOEmails"})">GLO Emails</a></li>
                                    <li><a href="@Url.Action("ManagerToolsEtc", "ManagerTools", new { OtherToolSelected = "GLOCountyList"})">GLO County List</a></li>
                                    <li><a href="@Url.Action("ManagerToolsEtc", "ManagerTools", new { OtherToolSelected = "CleanShotContact"})">Clean Shot Contact Info</a></li>
                                    <li><a href="@Url.Action("Logs", "ManagerTools", new { LogsSelected = "CleanShot"})">Clean Shot Incident Log</a></li>
                                    <li><a href="@Url.Action("Logs", "ManagerTools", new { LogsSelected = "GenInc"})">General Incident Log</a></li>
                                    <li><a href="@Url.Action("IDEditing", "ManagerTools", new { IDsSelected = "Incident"})">Incident IDs</a></li>
                                    <li><a href="@Url.Action("IDEditing", "ManagerTools", new { IDsSelected = "CleanShot"})">Clean Shot IDs</a></li>
                                    <li><a href="@Url.Action("IDEditing", "ManagerTools", new { IDsSelected = "BASFEnvironmental"})">BASF Environmental IDs</a></li>
                                    <li><a href="@Url.Action("IDEditing", "ManagerTools", new { IDsSelected = "BASFInjury"})">BASF Injury IDs</a></li>
                                    <li><a href="@Url.Action("Logs", "ManagerTools", new { LogsSelected = "Shell"})">Shell Report Log</a></li>
                                    <li><a href="@Url.Action("ManagerToolsEtc", "ManagerTools", new { OtherToolSelected = "ViewReports"})">View Reports</a></li>
                                    <li><a href="@Url.Action("ManagerToolsEtc", "ManagerTools", new { OtherToolSelected = "UserManagement"})">User Management</a></li>
                                </ul>
                            </div>
                        </li>
                        @if (Session["Username"] != null && Session["AccountType"] != null && Session["FullName"] != null && Session["Username"].ToString() != "" && Session["AccountType"].ToString() != "" && Session["FullName"].ToString() != "")
                        {
                            <li class="nav-item">
                                <a class="nav-link text-dark" href="@Url.Action("Logout", "Home")">Logout</a>
                            </li>
                        }
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2001-2022 VelocityEHS. All rights reserved. VelocityEHS® and VelocityEHS Accelerate® are proprietary trademarks of VelocityEHS. All other trademarks are the property of the respective owners.
        </div>
    </footer>
    <script src="~/scripts/jquery.min.js"></script>
    <script src="~/scripts/bootstrap.min.js"></script>
    <script src="~/scripts/site.js" asp-append-version="true"></script>
    @*@await RenderSectionAsync("Scripts", required: false)*@
</body>
</html>
